
(rules PCB stm32-mini12864
  (snap_angle 
    fortyfive_degree
  )
  (autoroute_settings
    (fanout off)
    (eu.mihosoft.freerouting.autoroute on)
    (postroute on)
    (vias on)
    (via_costs 50)
    (plane_via_costs 5)
    (start_ripup_costs 100)
    (start_pass_no 2151)
    (layer_rule F.Cu
      (active on)
      (preferred_direction horizontal)
      (preferred_direction_trace_costs 1.0)
      (against_preferred_direction_trace_costs 4.1)
    )
    (layer_rule B.Cu
      (active on)
      (preferred_direction vertical)
      (preferred_direction_trace_costs 1.0)
      (against_preferred_direction_trace_costs 1.3)
    )
  )
  (rule
    (width 250.0)
    (clear 200.2)
    (clear 125.0 (type smd_to_turn_gap))
    (clear 50.0 (type smd_smd))
  )
  (padstack "Via[0-1]_800:400_um"
    (shape
      (circle F.Cu 800.0 0.0 0.0)
    )
    (shape
      (circle B.Cu 800.0 0.0 0.0)
    )
    (attach off)
  )
  (via 
    "Via[0-1]_800:400_um" "Via[0-1]_800:400_um" default
  )
  (via 
    "Via[0-1]_800:400_um-kicad_default" "Via[0-1]_800:400_um" "kicad_default"
  )
  (via_rule
    default "Via[0-1]_800:400_um"
  )
  (via_rule
    "kicad_default" "Via[0-1]_800:400_um-kicad_default"
  )
  (class default
    (clearance_class default)
    (via_rule default)
    (rule
      (width 250.0)
    )
    (circuit 
      (use_layer F.Cu B.Cu)
    )
  )
  (class "kicad_default"
    /D+ "/D-" /~RESET /SWDCLK /BOOT0 "Net-(D6-Pad2)" "LCD_NEOPIXEL" "LCD_RST"
    "LCD_A0" "LCD_CS" "BTN_ENC" BUZZER "BTN_EN2" "BTN_EN1" KILL NEOPIXEL3V
    MOSI MISO SCK
    (clearance_class "kicad_default")
    (via_rule kicad_default)
    (rule
      (width 252.0)
    )
    (circuit 
      (use_layer F.Cu B.Cu)
    )
  )
  (class class1
    GND VBUS
    (clearance_class default)
    (via_rule default)
    (rule
      (width 401.0)
    )
    (circuit 
      (use_layer F.Cu B.Cu)
    )
  )
  (class class2
    +3V3
    (clearance_class default)
    (via_rule default)
    (rule
      (width 351.0)
    )
    (circuit 
      (use_layer F.Cu B.Cu)
    )
  )
)